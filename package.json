    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "ts-node": "^9.0.0",
    "tslib": "^2.2.0",
    "typescript": "~4.2.4",
    "wait-on": "^5.3.0"
  }
